#include<stdio.h>
int main(){
    int a=1,b=1,c,n;
    
    scanf("%d",&n);
    if(n<=0){
        printf("Invalid input! Number of terms must be at least 1.");
    }
    else if(n==1){
        printf("Fibonacci series: %d",n);
    }
    else{
       printf("Fibonacci series: %d %d",a,b);
       for(int i=3;i<=n;i++){
          c = a+b;
          a=b;
          b=c;
          printf(" %d",c);
        }
    }
    return 0;
}
