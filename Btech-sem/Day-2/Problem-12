#include<stdio.h>
int main(){
    int n,c=0;
    scanf("%d",&n);
    if(n<2){
        printf("Invalid input! Number should be greater than or equal to 2.");
    }
    else{
        for(int i=1;i<=n;i++){
            if(n%i==0){
                c++;
            }
        }
        if(c==2){
            printf("%d is a prime number",n);
        }
        else{
            printf("%d is not a prime number",n);
        }
    }
    return 0;
}
