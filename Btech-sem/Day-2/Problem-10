#include<stdio.h>
int main(){
    double cp,sp,p,l;
    scanf("%lf%lf",&cp,&sp);
    if(cp<0||sp<0){
        printf("Invalid input! prices cannot be negative.");
    }
    else{
        if(cp>sp){
            l=cp-sp;
            printf("loss:%0.2lf",l);
        }
        else if(cp<sp){
            p=sp-cp;
            printf("profit:%0.2lf",p);
        }
        else if(sp==cp){
            printf("No profit,no loss");
        }
    }
    return 0;
}
