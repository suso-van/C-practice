#include<stdio.h>
int main(){
    int n;
    scanf("%d",&n);
    int mat[n][n];
    if(n==0){
        printf("Invalid input! Matrix order must be positive");
    }
    else{
        for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            scanf("%d",&mat[i][j]);
        }
    }
    int mm=mat[0][0];
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            if(mat[i][j]>mm){
                mm = mat[i][j];
            }
        }
    }
    printf("Maximum element: %d\n",mm);
    }
    return 0;
}
