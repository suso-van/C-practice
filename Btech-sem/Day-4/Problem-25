#include<stdio.h>
int main(){
    int n;
    scanf("%d",&n);
    if(n==0){
        printf("Invalid input! Number of elements must be positive");
    }
    else{
        int arr[n];
        for(int i=0;i<n;i++){
            scanf("%d",&arr[i]);
        }
        for(int i=0;i<n-1;i+=2){
            int nw= arr[i];
            arr[i] = arr[i+1];
            arr[i+1]= nw;
        }
        for(int i=0;i<n;i++){
           printf("%d ",arr[i]); 
        }
        printf("\n");
    }
    return 0;
}
