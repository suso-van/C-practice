#include<stdio.h>
int main(){
    int n,trace=0;
   
    scanf("%d",&n);
    if(n==0){
        printf("Invalid input! Matrix order must be positive.");
    }
    else{
        int mat[n][n];
        for(int i=0;i<n;i++){
            for(int j=0;j<n;j++){
                scanf("%d",&mat[i][j]);
            }
        }
        for(int i=0;i<n;i++){
           trace += mat[i][i];
        }
        printf("Trace of the matrix: %d\n",trace);
    }
    return 0;
}
