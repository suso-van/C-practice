#include<stdio.h>
#include<math.h>
#define PI 3.141592653589793
#define ACCURACY 1e-6
double Radians(double x){
    return x*(PI/180);
}
double Taylor(double d){
    double t=d,s=d;
    int n=1;
    while(fabs(t)>ACCURACY){
        t*=-1*d*d/((2*n)*(2*n+1));
        s+=t;
        n++;
    }
    return s;
}
int main(){
    printf("d (degrees) sin(d)\n");
    printf("--------------------\n");
    for(int i=0;i<=360;i+=15){
        double r = Radians(i);
        double v = Taylor(r);
        printf("%3d           %.6f\n", i, v);
    }
    return 0;
}
